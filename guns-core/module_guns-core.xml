    <javac2 destdir="${guns-core.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.guns-core}/javac">
      <compilerarg line="${compiler.args.guns-core}"/>
      <bootclasspath refid="guns-core.module.bootclasspath"/>
      <classpath refid="guns-core.module.production.classpath"/>
      <src refid="guns-core.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.guns-core"/>
    </javac2>
    
    <copy todir="${guns-core.output.dir}">
      <fileset dir="${module.guns-core.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.guns-core.tests" depends="register.custom.compilers,compile.module.guns-core.production" description="compile module guns-core; test classes" unless="skip.tests"/>
  
  <target name="clean.module.guns-core" description="cleanup module">
    <delete dir="${guns-core.output.dir}"/>
    <delete dir="${guns-core.testoutput.dir}"/>
  </target>
</project>